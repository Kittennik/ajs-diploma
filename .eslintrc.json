{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-expressions": 0,
        "no-new": 0,
        "max-len": 0,
        "consistent-return": 0,
        "no-param-reassign": 0,
        "no-restricted-syntax": 0,
        "no-alert": 0,
        "no-unused-vars": 0,
        "no-shadow": 0,
        "new-cap": 0
    }
}